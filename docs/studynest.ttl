@prefix : <http://purl.org/studynest#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://purl.org/studynest#> .

<http://purl.org/studynest#> rdf:type owl:Ontology ;
                              dc:creator "Yan Oliveira Maia" ;
                              dc:title "Ontologia Studynest"@pt ,
                                       "Studynest Ontology"@en ;
                              dcterms:created "2026-02-18"^^xsd:date ;
                              dcterms:license <https://creativecommons.org/licenses/by/4.0/legalcode> ;
                              dcterms:language "pt" , "en" ;
                              dcterms:publisher "Universidade Federal do Espírito Santo"@en ;
                              dcterms:subject "educação"@en , "aprendizado colaborativo"@en , 
                                              "ontologia"@en , "estudantes"@en , "grupos de estudo"@en ;
                              owl:versionIRI <http://purl.org/studynest/1.0.0#> ;
                              owl:versionInfo "1.0.0" ;
                              vann:preferredNamespacePrefix "studynest" ;
                              vann:preferredNamespaceUri "http://purl.org/studynest#" ;
                              rdfs:comment """Ontologia para o sistema Studynest, uma plataforma mobile de colaboração acadêmica.
                     
                     Esta ontologia modela usuários, disciplinas, grupos de estudo, materiais e mensagens.
                     
                     This work is distributed under Creative Commons Attribution License CC BY 4.0."""@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/studynest#area
:area rdf:type owl:ObjectProperty ;
      rdfs:isDefinedBy <http://purl.org/studynest#> ;
      rdfs:domain :Disciplina ;
      rdfs:range :AreaType ;
      rdfs:comment "Área acadêmica da disciplina."@en ;
      rdfs:label "area"@en ,
                 "área"@pt .


###  http://purl.org/studynest#associadoADisciplina
:associadoADisciplina rdf:type owl:ObjectProperty ;
                      owl:inverseOf :possuiGrupo ;
                      rdfs:isDefinedBy <http://purl.org/studynest#> ;
                      rdfs:domain :GrupoEstudo ;
                      rdfs:range :Disciplina ;
                      rdfs:comment "Grupo de estudo está associado a uma disciplina."@en ;
                      rdfs:label "associado à disciplina"@pt ,
                                 "associated with course"@en .


###  http://purl.org/studynest#contemMaterial
:contemMaterial rdf:type owl:ObjectProperty ;
                owl:inverseOf :pertenceADisciplina ;
                rdfs:isDefinedBy <http://purl.org/studynest#> ;
                rdfs:domain :Disciplina ;
                rdfs:range :MaterialEstudo ;
                rdfs:comment "Disciplina contém materiais de estudo."@en ;
                rdfs:label "contains material"@en ,
                           "contém material"@pt .


###  http://purl.org/studynest#envia
:envia rdf:type owl:ObjectProperty ;
       owl:inverseOf :enviadaPor ;
       rdfs:isDefinedBy <http://purl.org/studynest#> ;
       rdfs:domain :Usuario ;
       rdfs:range :Mensagem ;
       rdfs:comment "Usuário envia uma mensagem."@en ;
       rdfs:label "envia"@pt ,
                  "sends"@en .


###  http://purl.org/studynest#enviadaPor
:enviadaPor rdf:type owl:ObjectProperty ;
            rdfs:isDefinedBy <http://purl.org/studynest#> ;
            rdfs:domain :Mensagem ;
            rdfs:range :Usuario ;
            rdfs:comment "Mensagem foi enviada por um usuário."@en ;
            rdfs:label "enviada por"@pt ,
                       "sent by"@en .


###  http://purl.org/studynest#genero
:genero rdf:type owl:ObjectProperty ;
        rdfs:isDefinedBy <http://purl.org/studynest#> ;
        rdfs:domain :Usuario ;
        rdfs:range :Genero ;
        rdfs:comment "Gênero do usuário."@en ;
        rdfs:label "gender"@en ,
                   "gênero"@pt .


###  http://purl.org/studynest#inscritoEm
:inscritoEm rdf:type owl:ObjectProperty ;
            rdfs:isDefinedBy <http://purl.org/studynest#> ;
            rdfs:domain :Usuario ;
            rdfs:range :Disciplina ;
            rdfs:comment "Usuário está inscrito em uma disciplina."@en ;
            rdfs:label "enrolled in"@en ,
                       "inscrito em"@pt .


###  http://purl.org/studynest#membroDe
:membroDe rdf:type owl:ObjectProperty ;
          rdfs:isDefinedBy <http://purl.org/studynest#> ;
          rdfs:domain :Usuario ;
          rdfs:range :GrupoEstudo ;
          rdfs:comment "Usuário é membro de um grupo de estudo."@en ;
          rdfs:label "member of"@en ,
                     "membro de"@pt .


###  http://purl.org/studynest#participacaoDisciplina
:participacaoDisciplina rdf:type owl:ObjectProperty ;
                        rdfs:isDefinedBy <http://purl.org/studynest#> ;
                        rdfs:domain :Usuario ;
                        rdfs:range :ParticipacaoDisciplina ;
                        rdfs:comment "Relação entre usuário e disciplina com papel específico."@en ;
                        rdfs:label "course participation"@en ,
                                   "participação em disciplina"@pt .


###  http://purl.org/studynest#participacaoGrupo
:participacaoGrupo rdf:type owl:ObjectProperty ;
                   rdfs:isDefinedBy <http://purl.org/studynest#> ;
                   rdfs:domain :Usuario ;
                   rdfs:range :ParticipacaoGrupo ;
                   rdfs:comment "Relação entre usuário e grupo com papel específico."@en ;
                   rdfs:label "group participation"@en ,
                              "participação em grupo"@pt .


###  http://purl.org/studynest#pertenceADisciplina
:pertenceADisciplina rdf:type owl:ObjectProperty ;
                     rdfs:isDefinedBy <http://purl.org/studynest#> ;
                     rdfs:domain :MaterialEstudo ;
                     rdfs:range :Disciplina ;
                     rdfs:comment "Material pertence a uma disciplina."@en ;
                     rdfs:label "belongs to course"@en ,
                                "pertence à disciplina"@pt .


###  http://purl.org/studynest#pertenceAGrupo
:pertenceAGrupo rdf:type owl:ObjectProperty ;
                owl:inverseOf :possuiMensagem ;
                rdfs:isDefinedBy <http://purl.org/studynest#> ;
                rdfs:domain :Mensagem ;
                rdfs:range :GrupoEstudo ;
                rdfs:comment "Mensagem pertence a um grupo de estudo."@en ;
                rdfs:label "belongs to group"@en ,
                           "pertence a grupo"@pt .


###  http://purl.org/studynest#possuiGrupo
:possuiGrupo rdf:type owl:ObjectProperty ;
             rdfs:isDefinedBy <http://purl.org/studynest#> ;
             rdfs:domain :Disciplina ;
             rdfs:range :GrupoEstudo ;
             rdfs:comment "Disciplina possui grupos de estudo."@en ;
             rdfs:label "has group"@en ,
                        "possui grupo"@pt .


###  http://purl.org/studynest#possuiMensagem
:possuiMensagem rdf:type owl:ObjectProperty ;
                rdfs:isDefinedBy <http://purl.org/studynest#> ;
                rdfs:domain :GrupoEstudo ;
                rdfs:range :Mensagem ;
                rdfs:comment "Grupo de estudo possui mensagens no chat."@en ;
                rdfs:label "has message"@en ,
                           "possui mensagem"@pt .


###  http://purl.org/studynest#possuiUsuario
:possuiUsuario rdf:type owl:ObjectProperty ;
               owl:inverseOf :vinculadoA ;
               rdfs:isDefinedBy <http://purl.org/studynest#> ;
               rdfs:domain :Instituicao ;
               rdfs:range :Usuario ;
               rdfs:comment "Instituição possui usuários vinculados."@en ;
               rdfs:label "has user"@en ,
                          "possui usuário"@pt .


###  http://purl.org/studynest#publica
:publica rdf:type owl:ObjectProperty ;
         owl:inverseOf :publicadoPor ;
         rdfs:isDefinedBy <http://purl.org/studynest#> ;
         rdfs:domain :Usuario ;
         rdfs:range :MaterialEstudo ;
         rdfs:comment "Usuário publica um material de estudo."@en ;
         rdfs:label "publica"@pt ,
                    "publishes"@en .


###  http://purl.org/studynest#publicadoPor
:publicadoPor rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy <http://purl.org/studynest#> ;
              rdfs:domain :MaterialEstudo ;
              rdfs:range :Usuario ;
              rdfs:comment "Material foi publicado por um usuário."@en ;
              rdfs:label "publicado por"@pt ,
                         "published by"@en .


###  http://purl.org/studynest#status
:status rdf:type owl:ObjectProperty ;
        rdfs:isDefinedBy <http://purl.org/studynest#> ;
        rdfs:domain :Mensagem ;
        rdfs:range :StatusMensagem ;
        rdfs:comment "Status da mensagem."@en ;
        rdfs:label "status"@en ,
                   "status"@pt .


###  http://purl.org/studynest#temDisciplina
:temDisciplina rdf:type owl:ObjectProperty ;
               rdfs:isDefinedBy <http://purl.org/studynest#> ;
               rdfs:domain :ParticipacaoDisciplina ;
               rdfs:range :Disciplina ;
               rdfs:label "has course"@en ,
                          "tem disciplina"@pt .


###  http://purl.org/studynest#temGrupo
:temGrupo rdf:type owl:ObjectProperty ;
          rdfs:isDefinedBy <http://purl.org/studynest#> ;
          rdfs:domain :ParticipacaoGrupo ;
          rdfs:range :GrupoEstudo ;
          rdfs:label "has group"@en ,
                     "tem grupo"@pt .


###  http://purl.org/studynest#temPapelDisciplina
:temPapelDisciplina rdf:type owl:ObjectProperty ;
                    rdfs:isDefinedBy <http://purl.org/studynest#> ;
                    rdfs:domain :ParticipacaoDisciplina ;
                    rdfs:range :PapelDisciplina ;
                    rdfs:label "has course role"@en ,
                               "tem papel na disciplina"@pt .


###  http://purl.org/studynest#temPapelGrupo
:temPapelGrupo rdf:type owl:ObjectProperty ;
               rdfs:isDefinedBy <http://purl.org/studynest#> ;
               rdfs:domain :ParticipacaoGrupo ;
               rdfs:range :PapelGrupo ;
               rdfs:label "has group role"@en ,
                          "tem papel no grupo"@pt .


###  http://purl.org/studynest#temUsuarioDisciplina
:temUsuarioDisciplina rdf:type owl:ObjectProperty ;
                      rdfs:isDefinedBy <http://purl.org/studynest#> ;
                      rdfs:domain :ParticipacaoDisciplina ;
                      rdfs:range :Usuario ;
                      rdfs:label "has user"@en ,
                                 "tem usuário"@pt .


###  http://purl.org/studynest#temUsuarioGrupo
:temUsuarioGrupo rdf:type owl:ObjectProperty ;
                 rdfs:isDefinedBy <http://purl.org/studynest#> ;
                 rdfs:domain :ParticipacaoGrupo ;
                 rdfs:range :Usuario ;
                 rdfs:label "has user"@en ,
                            "tem usuário"@pt .


###  http://purl.org/studynest#tipo
:tipo rdf:type owl:ObjectProperty ;
      rdfs:isDefinedBy <http://purl.org/studynest#> ;
      rdfs:domain :MaterialEstudo ;
      rdfs:range :TipoMaterial ;
      rdfs:comment "Tipo do material de estudo."@en ;
      rdfs:label "tipo"@pt ,
                 "type"@en .


###  http://purl.org/studynest#vinculadoA
:vinculadoA rdf:type owl:ObjectProperty ;
            rdfs:isDefinedBy <http://purl.org/studynest#> ;
            rdfs:domain :Usuario ;
            rdfs:range :Instituicao ;
            rdfs:comment "Usuário está vinculado a uma instituição de ensino."@en ;
            rdfs:label "affiliated with"@en ,
                       "vinculado à"@pt .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/studynest#conteudo
:conteudo rdf:type owl:DatatypeProperty ;
          rdfs:isDefinedBy <http://purl.org/studynest#> ;
          rdfs:domain :Mensagem ;
          rdfs:range xsd:string ;
          rdfs:comment "Conteúdo textual da mensagem."@en ;
          rdfs:label "content"@en ,
                     "conteúdo"@pt .


###  http://purl.org/studynest#dataEnvio
:dataEnvio rdf:type owl:DatatypeProperty ;
           rdfs:isDefinedBy <http://purl.org/studynest#> ;
           rdfs:domain :Mensagem ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "Data em que a mensagem foi enviada."@en ;
           rdfs:label "data de envio"@pt ,
                      "sent date"@en .


###  http://purl.org/studynest#dataPublicacao
:dataPublicacao rdf:type owl:DatatypeProperty ;
                rdfs:isDefinedBy <http://purl.org/studynest#> ;
                rdfs:domain :MaterialEstudo ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "Data em que o material foi publicado."@en ;
                rdfs:label "data de publicação"@pt ,
                           "publication date"@en .


###  http://purl.org/studynest#descricao
:descricao rdf:type owl:DatatypeProperty ;
           rdfs:isDefinedBy <http://purl.org/studynest#> ;
           rdfs:range xsd:string ;
           rdfs:comment "Descrição do grupo de estudo."@en ;
           rdfs:label "description"@en ,
                      "descrição"@pt .


###  http://purl.org/studynest#email
:email rdf:type owl:DatatypeProperty ;
       rdfs:isDefinedBy <http://purl.org/studynest#> ;
       rdfs:domain :Usuario ;
       rdfs:range xsd:string ;
       rdfs:comment "Email do usuário."@en ;
       rdfs:label "email"@en .


###  http://purl.org/studynest#nome
:nome rdf:type owl:DatatypeProperty ;
      rdfs:isDefinedBy <http://purl.org/studynest#> ;
      rdfs:range xsd:string ;
      rdfs:comment "Nome do usuário, instituição, disciplina, grupo ou material."@en ;
      rdfs:label "name"@en ,
                 "nome"@pt .


###  http://purl.org/studynest#senhaHash
:senhaHash rdf:type owl:DatatypeProperty ;
           rdfs:isDefinedBy <http://purl.org/studynest#> ;
           rdfs:domain :Usuario ;
           rdfs:range xsd:string ;
           rdfs:comment "Hash da senha do usuário."@en ;
           rdfs:label "password hash"@en ,
                      "senha hash"@pt .


###  http://purl.org/studynest#tamanho
:tamanho rdf:type owl:DatatypeProperty ;
         rdfs:isDefinedBy <http://purl.org/studynest#> ;
         rdfs:domain :MaterialEstudo ;
         rdfs:range xsd:long ;
         rdfs:comment "Tamanho do arquivo em bytes."@en ;
         rdfs:label "size"@en ,
                    "tamanho"@pt .


###  http://purl.org/studynest#telefone
:telefone rdf:type owl:DatatypeProperty ;
          rdfs:isDefinedBy <http://purl.org/studynest#> ;
          rdfs:domain :Usuario ;
          rdfs:range xsd:string ;
          rdfs:comment "Telefone do usuário."@en ;
          rdfs:label "phone"@en ,
                     "telefone"@pt .


#################################################################
#    Classes
#################################################################

###  http://purl.org/studynest#AreaType
:AreaType rdf:type owl:Class ;
          rdfs:isDefinedBy <http://purl.org/studynest#> ;
          rdfs:comment "Enumeração de áreas acadêmicas para disciplinas."@en ;
          rdfs:label "Area Type"@en ,
                     "Área"@pt .


###  http://purl.org/studynest#Disciplina
:Disciplina rdf:type owl:Class ;
            rdfs:isDefinedBy <http://purl.org/studynest#> ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :contemMaterial ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :possuiGrupo ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger
                            ] ;
            rdfs:comment "Representa uma disciplina acadêmica."@en ;
            rdfs:label "Course"@en ,
                       "Disciplina"@pt .


###  http://purl.org/studynest#Genero
:Genero rdf:type owl:Class ;
        rdfs:isDefinedBy <http://purl.org/studynest#> ;
        rdfs:comment "Enumeração de gêneros possíveis para um usuário."@en ;
        rdfs:label "Gender"@en ,
                   "Gênero"@pt .


###  http://purl.org/studynest#GrupoEstudo
:GrupoEstudo rdf:type owl:Class ;
             rdfs:isDefinedBy <http://purl.org/studynest#> ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :possuiMensagem ;
                               owl:minCardinality "0"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :associadoADisciplina ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ;
             rdfs:comment "Representa um grupo de estudo formado por usuários."@en ;
             rdfs:label "Grupo de Estudo"@pt ,
                        "Study Group"@en .


###  http://purl.org/studynest#Instituicao
:Instituicao rdf:type owl:Class ;
             rdfs:isDefinedBy <http://purl.org/studynest#> ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :possuiUsuario ;
                               owl:minCardinality "0"^^xsd:nonNegativeInteger
                             ] ;
             rdfs:comment "Instituição de ensino à qual o usuário pode estar vinculado."@en ;
             rdfs:label "Instituição"@pt ,
                        "Institution"@en .


###  http://purl.org/studynest#MaterialEstudo
:MaterialEstudo rdf:type owl:Class ;
                rdfs:isDefinedBy <http://purl.org/studynest#> ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :pertenceADisciplina ;
                                  owl:cardinality "1"^^xsd:nonNegativeInteger
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :publicadoPor ;
                                  owl:cardinality "1"^^xsd:nonNegativeInteger
                                ] ;
                rdfs:comment "Representa um material de estudo (arquivo, resumo, etc.) compartilhado."@en ;
                rdfs:label "Material de Estudo"@pt ,
                           "Study Material"@en .


###  http://purl.org/studynest#Mensagem
:Mensagem rdf:type owl:Class ;
          rdfs:isDefinedBy <http://purl.org/studynest#> ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :enviadaPor ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :pertenceAGrupo ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          rdfs:comment "Representa uma mensagem trocada no chat de um grupo."@en ;
          rdfs:label "Mensagem"@pt ,
                     "Message"@en .


###  http://purl.org/studynest#PapelDisciplina
:PapelDisciplina rdf:type owl:Class ;
                 rdfs:isDefinedBy <http://purl.org/studynest#> ;
                 rdfs:comment "Papel que um usuário pode desempenhar em uma disciplina."@en ;
                 rdfs:label "Course Role"@en ,
                            "Papel na Disciplina"@pt .


###  http://purl.org/studynest#PapelGrupo
:PapelGrupo rdf:type owl:Class ;
            rdfs:isDefinedBy <http://purl.org/studynest#> ;
            rdfs:comment "Papel que um usuário pode desempenhar em um grupo de estudo."@en ;
            rdfs:label "Group Role"@en ,
                       "Papel no Grupo"@pt .


###  http://purl.org/studynest#ParticipacaoDisciplina
:ParticipacaoDisciplina rdf:type owl:Class ;
                        rdfs:isDefinedBy <http://purl.org/studynest#> ;
                        rdfs:comment "Representa a relação entre usuário e disciplina, incluindo o papel desempenhado."@en ;
                        rdfs:label "Course Participation"@en ,
                                   "Participação em Disciplina"@pt .


###  http://purl.org/studynest#ParticipacaoGrupo
:ParticipacaoGrupo rdf:type owl:Class ;
                   rdfs:isDefinedBy <http://purl.org/studynest#> ;
                   rdfs:comment "Representa a relação entre usuário e grupo de estudo, incluindo o papel desempenhado."@en ;
                   rdfs:label "Group Participation"@en ,
                              "Participação em Grupo"@pt .


###  http://purl.org/studynest#StatusMensagem
:StatusMensagem rdf:type owl:Class ;
                rdfs:isDefinedBy <http://purl.org/studynest#> ;
                rdfs:comment "Enumeração de status possíveis para uma mensagem."@en ;
                rdfs:label "Message Status"@en ,
                           "Status da Mensagem"@pt .


###  http://purl.org/studynest#TipoMaterial
:TipoMaterial rdf:type owl:Class ;
              rdfs:isDefinedBy <http://purl.org/studynest#> ;
              rdfs:comment "Enumeração de tipos de material de estudo."@en ;
              rdfs:label "Material Type"@en ,
                         "Tipo de Material"@pt .


###  http://purl.org/studynest#Usuario
:Usuario rdf:type owl:Class ;
         rdfs:isDefinedBy <http://purl.org/studynest#> ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :vinculadoA ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ;
         rdfs:comment "Representa um usuário do sistema Studynest."@en ;
         rdfs:label "User"@en ,
                    "Usuário"@pt .


#################################################################
